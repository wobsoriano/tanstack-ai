  "dependencies": {
    "@tanstack/ai-client": "workspace:*",
    "@tanstack/ai-solid": "workspace:*",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "solid-markdown": "^2.1.0"
  },
  "peerDependencies": {
    "@tanstack/ai-client": "workspace:*",
    "@tanstack/ai-solid": "workspace:*",
    "solid-js": ">=1.9.7"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.14",
