{
  "name": "@tanstack/ai-solid-ui",
  "version": "0.0.0",
  "description": "Headless Solid components for building AI chat interfaces",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TanStack/ai.git",
    "directory": "packages/typescript/ai"
  },
  "exports": {
    ".": {
      "solid": "./src/index.ts",
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "vite build",
